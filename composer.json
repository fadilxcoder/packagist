{
    "name": "fadil/packagist",
    "require": {
        "tracy/tracy": "^2.8",
        "fzaninotto/faker": "^1.9"
    },
    "extra": {
        "hooks": {
            "pre-commit": [
                "echo committing as $(git config user.name)"
            ],
            "commit-msg": "grep -q '[A-Z]+-[0-9]+.*' $1",
            "pre-push": [
                "echo prepush..."
            ],
            "post-merge": "composer -v"
        }
    },
    "require-dev": {
        "brainmaestro/composer-git-hooks": "^2.8"
    }
}
